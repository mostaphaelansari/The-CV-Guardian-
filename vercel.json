{
    "version": 2,
    "builds": [
        {
            "src": "src/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*\\.(css|js|html|png|jpg|jpeg|svg|ico))",
            "dest": "/public/$1"
        },
        {
            "src": "/",
            "dest": "/public/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/src/app.js"
        }
    ]
}